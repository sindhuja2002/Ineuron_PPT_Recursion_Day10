#question 8
def countConsonants(string):
    consonants = "bcdfghjklmnpqrstvwxyzBCDFGHJKLMNPQRSTVWXYZ"
    count = 0
    for char in string:
        if char in consonants:
            count += 1
    return count

# Example usage
str1 = "abc de"
print(countConsonants(str1))  # Output: 3

str2 = "geeksforgeeks portal"
print(countConsonants(str2))  # Output: 12

#TC - O(n)
#SC - O(1)
